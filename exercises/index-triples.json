{
  "name": "triples-index",
  "modules": {
      "indexTriples": {
          "testIndexTriples": {
              "arguments": "a list of triples",
              "returns": "returns the list of indexTriple() return values",
              "tests": [
                  {
                      "inputs": [ [ ["a", "b", "c"], ["a", "b", "a"] ] ],
                      "output": [ true, true ]
                  },
                  {
                      "inputs": [ [ ["a", "b", "c"], ["a", "b", "a"], ["a", "b", "c"] ] ],
                      "output": [ true, true, false ]
                  }
               ]
            },
          "testFetchTriples": {
            "arguments": "a query and a list of triples",
            "returns": "returns the shortest indexed list that includes all triples that might match query",
            "tests": [
                {
                    "inputs": [ ["?x", "b", "?x"], [ ["a", "b", "c"], ["a", "b", "a"] ] ],
                    "output": [ ["a", "b", "c"], ["a", "b", "a"] ]
                },
                {
                    "inputs": [ ["?x", "b", "d"], [ ["a", "b", "c"], ["a", "b", "a"] ]  ],
                    "output": [ ]
                },
                {
                    "inputs": [ ["?x", "b", "c"], [ ["a", "b", "c"], ["a", "b", "a"] ]  ],
                    "output": [ ["a", "b", "c"] ]
                }
             ]
          }
        }
    }
}
